<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="FoodItemStyle" >
        <item name="android:background">@color/green62</item>
        <item name="android:textColor">@color/white</item>

    </style>
    <style name="FizItemStyle" >
        <item name="android:background">@color/blue62</item>
        <item name="android:textColor">@color/white</item>
        <item name="cornerRadius">12dp</item>

    </style>
    <style name="NavigationButtomButton" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:backgroundTint">#0000FF00</item>
        <item name="android:textColor">@color/black</item>
        <item name="cornerRadius">0dp</item>
        <item name="fontFamily">sans-serif-light</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="iconTint">@color/black</item>
        <item name="iconGravity">textTop</item>
        <item name="iconPadding">3dp</item>
        <item name="android:ellipsize">none</item>
        <item name="singleLine">true</item>
        <item name="android:letterSpacing">0</item>
</style>
    <style name="BackButtonStyle" parent="Widget.MaterialComponents.Button">
        <item name="android:stateListAnimator">@null</item>
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="icon">@drawable/baseline_arrow_back_ios_new_24</item>
        <item name="android:text">_</item>
        <item name="iconTint">@color/black</item>
        <item name="backgroundTint">@color/white</item>
    </style>
    <style name="SearchEditText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColorHint">@color/gray</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingVertical">8.5dp</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="drawableTint">@color/gray</item>
    </style>
    <style name="addEatingBigButton">
        <item name="cornerRadius">8dp</item>
        <item name="textAllCaps">false</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:backgroundTint">@color/blue62</item>
    </style>

</resources>